{
  "name": "@apis-games-front/monorepo",
  "version": "0.0.0",
  "workspaces": [
    "packages/*",
    "tools/*",
    "games/*"
  ],
  "private": true,
  "author": "Apis team",
  "license": "MIT",
  "engines": {
    "node": "^20.15.0"
  },
  "scripts": {
    "lint": "yarn concurrently --timings 'npm:lint:*'",
    "lint:eslint": "eslint",
    "lint:styles": "stylelint '**/*.{scss,css}'",
    "lint:tsc": "tsc --noEmit --pretty --project .",
    "quality": "yarn concurrently --timings --kill-others-on-fail 'yarn depcheck' 'yarn lint' 'yarn npm audit --all --recursive'",
    "build:plinko": "yarn workspace @apis-games-front/plinko build",
    "start:plinko": "yarn workspace @apis-games-front/plinko start",
    "updatel10n:plinko": "yarn workspace @apis-games-front/plinko updatel10n",
    "postinstall": "yarn workspaces foreach --all -p -t run build"
  },
  "dependencies": {
